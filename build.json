{
  "name": "EarthView Wallpapers",
  "version": "0.0.1",
  "identifier": "me.dags.ev",
  "icon": "assets/icon.png",
  "output": "builds",
  "setup": [
    "go install github.com/GeertJohan/go.rice",
    "rice embed"
  ],
  "targets": {
    "linux/amd64": {
      "meta": {
        "categories": "Utility"
      }
    },
    "darwin/amd64": {},
    "windows/amd64": {
      "compress": false,
      "generate": ["goversioninfo -64"],
      "flags": ["-H=windowsgui"]
    }
  }
}